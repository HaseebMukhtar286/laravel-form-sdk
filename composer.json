{
  "name": "haseebmukhtar286/laravel-form-sdk",
  "description": "Make dynamic CRUD using MongoDB",
  "keywords": [
    "haseebmukhtar286",
    "laravel-form-sdk"
  ],
  "homepage": "https://github.com/haseebmukhtar286/laravel-form-sdk",
  "license": "MIT",
  "type": "library",
  "authors": [
    {
      "name": "Haseeb Mukhtar",
      "email": "haseebmuhtar165@gmail.com",
      "role": "Developer"
    }
  ],
  "require": {
    "php": "^7.4|^8.0|^8.1",
    "illuminate/support": "^5.0|^6.0|^7.0|^8.0|^9.0|^10.0"
  },
  "require-dev": {
    "orchestra/testbench": "^6.0",
    "phpunit/phpunit": "^9.0"
  },
  "autoload": {
    "psr-4": {
      "haseebmukhtar286\\LaravelFormSdk\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "haseebmukhtar286\\LaravelFormSdk\\Tests\\": "tests"
    }
  },
  "scripts": {
    "test": "vendor/bin/phpunit",
    "test-coverage": "vendor/bin/phpunit --coverage-html coverage"
  },
  "config": {
    "sort-packages": true
  },
  "extra": {
    "laravel": {
      "providers": [
        "haseebmukhtar286\\LaravelFormSdk\\LaravelFormSdkServiceProvider"
      ],
      "aliases": {
        "LaravelFormSdk": "haseebmukhtar286\\LaravelFormSdk\\LaravelFormSdkFacade"
      }
    }
  }
}
