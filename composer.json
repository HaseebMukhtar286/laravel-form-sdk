{
    "name": "ascend/efficax-builder",
    "description": "Efficax form ",
    "keywords": [
        "ascend",
        "efficax-builder"
    ],
    "homepage": "https://github.com/ascend/efficax-builder",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Hassan AAmir",
            "email": "hassan.aamir@ascend.com.sa",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.4|^8.0",
        "illuminate/support": "^8.0"
    },
    "require-dev": {
        "orchestra/testbench": "^6.0",
        "phpunit/phpunit": "^9.0"
    },
    "autoload": {
        "psr-4": {
            "Ascend\\EfficaxBuilder\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Ascend\\EfficaxBuilder\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"

    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Ascend\\EfficaxBuilder\\EfficaxBuilderServiceProvider"
            ],
            "aliases": {
                "EfficaxBuilder": "Ascend\\EfficaxBuilder\\EfficaxBuilderFacade"
            }
        }
    }
}
